<section class="Contact-us">
        <div class="Contact-us-image">
            <img class="Float-Left"
                src="/images/studentmanager.jpeg"
                alt="gradhat" srcset="">
        </div>
       
        <div class="Contact-us-Content">

            <div class="Contact-us-Title">
                <h2 class ="center">Contact Form</h2>
            </div>

            <div data-visible="true"class="container">
                <form >
                    <label for="firstName">First Name</label>
                    <input type="text" id="firstName" name="firstName" placeholder="First Name">

                    <label for="lastName">Last Name</label>
                    <input type="text" id="lastName" name="lastName" placeholder="Last Name">
                    <label for="email">E-Mail Address</label>
                    <input type="text" id="email" name="emailaddress" placeholder="Email Address">

                    <label for="subject">Message</label>
                    <textarea id="message" name="subject" placeholder="Leave us a message."
                        style="height:150px"></textarea>

                   <button  class="Submit-Form">Submit</button>

                        <!-- <input type="submit" value="Submit"> -->
                </form>
            </div>
        </div>
        </section>

                <script src="./js/contact.js"></script>
                